{  
    "scopeName": "code.clog",
    "fileTypes": ["clog"],
    "foldingStartMarker": ".*╔",
    "foldingStopMarker": ".*╚",
    "patterns": [
        {
            "match": "(^[0-9]*)(.*?)([0-9]*) \\[(.*?)\\]::\\[(.*?)\\]::\\[(.*?)\\](.*)",
            "captures": {
                "1": {"name": "constant.numeric"},
                "2": {"name": "markup.quote"},
                "3": {"name": "entity.name.type" },
                "4": {"name": "markup.bold" },
                "5": {"name": "markup.bold" },
                "6": {"name": "markup.bold" },
                "7": {"name": "constant.italic"}
            }
        },
        {
            "match": "(^[0-9]*)(.*?)([0-9]*) \\[(.*?)\\](.*?:)(.*)",
            "captures": {
                "1": {"name": "constant.numeric"},
                "2": {"name": "markup.quote"},
                "3": {"name": "entity.name.type" },
                "4": {"name": "markup.bold" },
                "5": {"name": "constant.italic" },
                "6": {"name": "comment.line" }
            }
        }
    ]
}